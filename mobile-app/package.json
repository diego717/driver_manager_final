{
  "name": "mobile-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start",
    "dev:remote": "npm --prefix .. run dev:remote",
    "deploy:worker": "npm --prefix .. run deploy",
    "d1:migrate": "npm --prefix .. run d1:migrate",
    "d1:migrate:remote": "npm --prefix .. run d1:migrate:remote",
    "users:sync:r2-to-d1": "npm --prefix .. run users:sync:r2-to-d1 --",
    "d1:check:web-users": "npx --prefix .. wrangler --cwd=.. d1 execute driver-manager-db --remote --command \"SELECT name FROM sqlite_master WHERE type='table' AND name='web_users';\"",
    "d1:check:web-users-schema": "npx --prefix .. wrangler --cwd=.. d1 execute driver-manager-db --remote --command \"PRAGMA table_info(web_users);\"",
    "d1:check:web-users-count": "npx --prefix .. wrangler --cwd=.. d1 execute driver-manager-db --remote --command \"SELECT COUNT(*) AS total_users FROM web_users;\"",
    "android": "expo run:android",
    "build:android:apk": "npx eas-cli build --platform android --profile preview",
    "build:android:aab": "npx eas-cli build --platform android --profile production",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "vitest run"
  },
  "dependencies": {
    "@expo/vector-icons": "^15.0.3",
    "@react-navigation/native": "^7.1.8",
    "axios": "^1.13.5",
    "crypto-js": "^4.2.0",
    "driver-manager-db": "file:..",
    "expo": "~54.0.33",
    "expo-constants": "~18.0.13",
    "expo-file-system": "~19.0.21",
    "expo-font": "~14.0.11",
    "expo-image-manipulator": "~14.0.8",
    "expo-image-picker": "~17.0.10",
    "expo-linking": "~8.0.11",
    "expo-local-authentication": "~17.0.8",
    "expo-notifications": "~0.32.16",
    "expo-router": "~6.0.23",
    "expo-secure-store": "~15.0.8",
    "expo-splash-screen": "~31.0.13",
    "expo-status-bar": "~3.0.9",
    "expo-web-browser": "~15.0.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-native": "0.81.5",
    "react-native-reanimated": "~4.1.1",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-web": "~0.21.0",
    "react-native-worklets": "0.5.1"
  },
  "devDependencies": {
    "@types/crypto-js": "^4.2.2",
    "@types/react": "~19.1.0",
    "react-test-renderer": "19.1.0",
    "typescript": "~5.9.2",
    "vitest": "^3.2.4"
  },
  "private": true
}
